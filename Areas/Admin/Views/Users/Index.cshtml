@model IEnumerable<AppUser>

@{
    ViewData["Title"] = "Users";
}
<br/>
<h1>Users</h1>
<br/>
<table class="table">
    <thead>
        <tr class="table-primary">
            <th>
                Username
            </th>
            <th>
                Email
            </th>
        </tr>
    </thead>
@foreach (var item in Model) {
    <tbody>
        <tr class="table-light">
            <td>
                @item.UserName
            </td>
            <td>
                @item.Email
            </td>
        </tr>
    </tbody>
}
</table>
